(section
  (ref "cgbc_bounded_stacks_is_empty")
  (title "CGBC.Bounded_Stacks.Is_Empty")
  (para function_desc "Stack is empty?")
  (subsection
    (title "Synopsis")
    (para-verbatim function_synopsis
"function Is_Empty (Container : in Stack) return Boolean;
"))
  (subsection
    (title "Description")
    (para
      "The " (item ada_function "Is_Empty") " function
      is equivalent to:")
    (para-verbatim variable "(Length (Container) = 0)"))
  (subsection
    (title "Performance/Complexity")
    (para
      "The " (item ada_function "Is_Empty") " function executes
       in " (item constant "O(1)") " time.")))
